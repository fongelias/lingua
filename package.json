{
  "name": "lingua",
  "version": "0.0.1",
  "description": "nlp lib",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "clean": "rm -r dist",
    "doc": "typedoc --options .typedoc.json",
    "doc:clean": "rm -r doc",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fongelias/lingua.git"
  },
  "author": "Elias Fong <fongelias@gmail.com> (https://fongelias.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fongelias/lingua/issues"
  },
  "homepage": "https://github.com/fongelias/lingua#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "mocha": "^5.2.0",
    "rollup": "^0.65.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "ts-jest": "^23.1.4",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  }
}
